  "dependencies": {
    "npm": "^6.9.0",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.9",
    "vue-class-component": "^6.3.2",
    "vue-property-decorator": "^7.3.0",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "@types/nock": "^9.3.1",
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-pwa": "^3.5.1",
    "@vue/cli-plugin-typescript": "^3.5.1",
    "@vue/cli-plugin-unit-jest": "^3.5.1",
    "@vue/cli-service": "^3.5.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "axios": "^0.18.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.14",
    "dotaconstants": "^6.16.0",
    "gh-pages": "^1.2.0",
    "lint-staged": "^6.0.0",
    "nock": "^9.6.1",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.3.3333",
    "vue-template-compiler": "^2.6.9",
    "vuex-class": "^0.3.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "homepage": "https://dricholm.github.io/guess-the-hero"
